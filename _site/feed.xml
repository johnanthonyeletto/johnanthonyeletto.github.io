&lt;span class=&quot;ss&quot;&gt;employer: &lt;/span&gt;&lt;span class=&quot;s2&quot;&gt;&quot;Town of Hempstead&quot;&lt;/span&gt;
&lt;span class=&quot;ss&quot;&gt;dates: &lt;/span&gt;&lt;span class=&quot;s2&quot;&gt;&quot;June 2013-September 2014&quot;&lt;/span&gt;
&lt;span class=&quot;ss&quot;&gt;categories: &lt;/span&gt;&lt;span class=&quot;n&quot;&gt;job&lt;/span&gt;
